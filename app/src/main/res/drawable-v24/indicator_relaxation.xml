<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:drawable="@drawable/wyl"
        android:width="99dp"
        android:height="50dp"
        android:left="10dp"
        android:right="10dp"
        android:gravity="center">
    <shape>
        <padding
            android:bottom="6px"
            android:left="2px"
            android:right="0dp"
            android:top="0dp"/>
        <solid
            android:color="#00000000"
            />
        <corners android:radius="20dp">
        </corners>
        <stroke
            android:width="1dp"
            android:color="#BBBBBB"
            />
    </shape>
</item>
</layer-list>
